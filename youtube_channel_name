TR=(
   "${ciencia_de_dados[@]}"
   "${cloud[@]}"
   "${computacao_gringos[@]}"
   "${dinossauros[@]}"
   "${distros[@]}"
   "${elixir[@]}"
   "${golang[@]}"
   "${hardware[@]}"
   "${julia[@]}"
   "${linux[@]}"
   "${programacao[@]}"
   "${python[@]}"
   "${rust[@]}"
   "${seguranca[@]}"
   "${shell[@]}"
   "${tech[@]}"
)

HT=(
   "${animes[@]}"
   "${aviacao[@]}"
   "${ciencia[@]}"
   "${cinema[@]}"
   "${conferencias[@]}"
   "${cursos[@]}"
   "${direito[@]}"
   "${engenharia[@]}"
   "${filosofia[@]}"
   "${futebol[@]}"
   "${games[@]}"
   "${kurz[@]}"
   "${literatura[@]}"
   "${luta[@]}"
   "${metaverse[@]}"
   "${music[@]}"
   "${noticias_brasil[@]}"
   "${noticias_internacionais[@]}"
   "${parkour[@]}"
   "${surf[@]}"
   "${ted[@]}"
   "${universidade[@]}"
   "${variedades[@]}"
   "${vasco[@]}"
)

function _show_tr() {
    for user in "${TR[@]}"; do
        # ID -> "${user%% *}"
        # NAME -> "${user#* }"
        printf "%s\n" "${user#* }"
    done
}

function _show_ht() {
    for user in "${HT[@]}"; do
        # ID -> "${user%% *}"
        # NAME -> "${user#* }"
        printf "%s\n" "${user#* }"
    done
}

function _get_id_name() {
    # THIS_FILE="./youtube_channel_name"
    THIS_FILE="${2}"
    grep -E -m 1 "${1}" "${THIS_FILE}" | cut -f2 -d\"
    # grep -E -m 1 "${1}" "${THIS_FILE}" | cut -f6- -d" "
    # grep -E -m 1 "${1}" "${THIS_FILE}" | sed -e "s/\"[a-zA-Z0-9\_-]\+\s\(.*\)\s(.*).*\"/\1/g"
}


